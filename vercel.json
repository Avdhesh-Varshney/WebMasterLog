{
    "version": 2,
    "builds": [
        {
            "src": "src/app/next/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/app/next/$1"
        }
    ],
    "cleanUrls": true,
    "redirects": [
        {
            "source": "/",
            "destination": "/src/app/next",
            "statusCode": 301
        }
    ]
}